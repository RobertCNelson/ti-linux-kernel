		/* NAVSS512_IR_IN_490 -> NAVSS512_IR_OUT_189 -> GIC_SPI_733 */
		interrupts = <GIC_SPI 701 IRQ_TYPE_LEVEL_HIGH>;

		mbox_mcu0_r5f0: mbox-mcu0-r5f0 {
			ti,mbox-rx = <0 0 0>;
			ti,mbox-tx = <1 0 0>;
