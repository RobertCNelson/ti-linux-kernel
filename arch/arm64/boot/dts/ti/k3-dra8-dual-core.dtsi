&cpus {
	cpu-map {
		cluster0: cluster0 {
			core0 {
				cpu = <&cpu0>;
			};

			core1 {
				cpu = <&cpu1>;
			};
		};

	};

	/* CPU0 is already defined */

	cpu1: cpu@1 {
		compatible = "arm,cortex-a72","arm,armv8";
		reg = <0x001>;
		device_type = "cpu";
		enable-method = "psci";
		next-level-cache = <&L2_0>;
	};
};
