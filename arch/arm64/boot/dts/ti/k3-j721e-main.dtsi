			interrupt-parent = <&main_navss_intr>;
			interrupts = <216 0 IRQ_TYPE_LEVEL_HIGH>;

			mbox_main_r5fss1_core0: mbox-main-r5fss1-core0 {
				ti,mbox-rx = <0 0 0>;
				ti,mbox-tx = <1 0 0>;
