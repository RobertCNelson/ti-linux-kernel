&cpus {
	cpu-map {
		cluster0: cluster0 {
			core0 {
				cpu = <&cpu0>;
			};

			core1 {
				cpu = <&cpu1>;
			};
		};

		cluster1: cluster1 {
			core0 {
				cpu = <&cpu2>;
			};

			core1 {
				cpu = <&cpu3>;
			};
		};
	};

	/* CPU0 already defined */

	cpu1: cpu@1 {
		compatible = "arm,cortex-a53","arm,armv8";
		reg = <0x001>;
		device_type = "cpu";
		enable-method = "psci";
		next-level-cache = <&L2_0>;
	};

	cpu2: cpu@100 {
		compatible = "arm,cortex-a53","arm,armv8";
		reg = <0x100>;
		device_type = "cpu";
		enable-method = "psci";
		next-level-cache = <&L2_1>;
	};

	cpu3: cpu@101 {
		compatible = "arm,cortex-a53","arm,armv8";
		reg = <0x101>;
		device_type = "cpu";
		enable-method = "psci";
		next-level-cache = <&L2_1>;
	};
};

&soc0 {
	L2_1: l2-cache1 {
		compatible = "cache";
		cache-level = <2>;
		next-level-cache = <&msmc_l3>;
	};
};
